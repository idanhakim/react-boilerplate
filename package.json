{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "react-boilerplate",
  "main": "src/server.jsx",
  "scripts": {
    "start": "npm run start:client & npm run start:server",
    "start:client": "cross-env NODE_ENV=development webpack-serve --config webpack.config.serve.js",
    "start:server": "npm run watch:server & nodemon dist/server.js",
    "clean": "rimraf dist/",
    "build": "npm run clean && npm run build:server & npm run build:client",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack.config.server.js",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack.config.client.js",
    "lint": "eslint eslint --ext js,jsx src",
    "test": "jest",
    "deploy:test": "npm run lint && npm run test",
    "deploy:build": "npm run deploy:test && npm run build && docker build -t yuri/react-bo . ",
    "deploy:run": "docker run yuri/react-bo --hostname=yuri -p 49170:5000 -d ",
    "watch:server": "cross-env NODE_ENV=development webpack --config webpack.config.server.js -w",
    "dist": "npm run build && cd dist && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yurikrupnik/react-boilerplate.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yurikrupnik/react-boilerplate/issues"
  },
  "homepage": "https://github.com/yurikrupnik/react-boilerplate#readme",
  "dependencies": {
    "@material-ui/core": "^1.2.3",
    "axios": "^0.18.0",
    "faker": "^4.1.0",
    "flexboxgrid": "^6.3.1",
    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.1",
    "koa-favicon": "^2.0.1",
    "koa-logger": "^3.2.0",
    "koa-render-view": "^1.0.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "mongoose": "^5.1.6",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-form": "^3.5.5",
    "react-loadable": "^5.4.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "socket.io-logger": "0.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "connect-history-api-fallback": "^1.5.0",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "file-loader": "^1.1.11",
    "generate-json-webpack-plugin": "^0.3.1",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.18.0",
    "jest": "^23.1.0",
    "koa-connect": "^2.0.1",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.5",
    "optimize-css-assets-webpack-plugin": "^4.0.2",
    "raw-loader": "^0.5.1",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.6",
    "webpack": "^4.12.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.8",
    "webpack-node-externals": "^1.7.2",
    "webpack-serve": "^1.0.4"
  }
}
